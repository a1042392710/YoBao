<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@color/white"
    android:orientation="vertical">

    <TextView
        android:id="@+id/item_tv_title"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center_vertical"
        android:paddingLeft="18dp"
        android:paddingTop="14dp"
        android:paddingBottom="14dp"
        android:text="2019年2月佣金记录"
        android:textColor="@color/text_black33"
        android:textSize="13sp" />

    <include layout="@layout/view_liner_1dp" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">
        <TextView
            android:id="@+id/item_tv_label_three"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:background="#f8f8f8"
            android:gravity="center"
            android:paddingTop="13dp"
            android:paddingBottom="16dp"
            android:text="直推收益"
            android:textColor="#555555"
            android:textSize="12sp" />


        <TextView
            android:id="@+id/item_tv_label_two"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:gravity="center"
            android:paddingTop="13dp"
            android:paddingBottom="16dp"
            android:text="被动收益"
            android:textColor="#555555"
            android:textSize="12sp" />

        <TextView
            android:id="@+id/item_tv_label_one"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:background="#f8f8f8"
            android:gravity="center"
            android:paddingTop="13dp"
            android:paddingBottom="16dp"
            android:text="网体收益"
            android:textColor="#555555"
            android:textSize="12sp" />

        <TextView
            android:id="@+id/item_tv_label_four"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:background="@color/white"
            android:gravity="center"
            android:paddingTop="13dp"
            android:paddingBottom="16dp"
            android:text="总佣金"
            android:textColor="#555555"
            android:textSize="12sp" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/item_tv_label_content_three"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:gravity="center"
            android:paddingTop="13dp"
            android:paddingBottom="16dp"
            android:text="0"
            android:textColor="@color/red_fe8977"
            android:textSize="12sp" />

        <TextView
            android:id="@+id/item_tv_label_content_two"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:gravity="center"
            android:paddingTop="13dp"
            android:paddingBottom="16dp"
            android:text="0"
            android:textColor="@color/red_fe8977"
            android:textSize="12sp" />



        <TextView
            android:id="@+id/item_tv_label_content_one"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:gravity="center"
            android:paddingTop="13dp"
            android:paddingBottom="16dp"
            android:text="0"
            android:textColor="@color/red_fe8977"
            android:textSize="12sp" />
        <TextView
            android:id="@+id/item_tv_label_content_four"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:gravity="center"
            android:paddingTop="13dp"
            android:paddingBottom="16dp"
            android:text="￥0"
            android:textColor="@color/text_red_f76060"
            android:textSize="12sp" />
    </LinearLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="5dp"
        android:background="@color/baseBackGround" />

</LinearLayout>